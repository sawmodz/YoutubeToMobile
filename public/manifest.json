{
  "manifest_version": 2,

  "name": "YoutubeToMobile",
  "description": "Cette extension sert a recuperer la video que l'on regarde sur son mobile",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}